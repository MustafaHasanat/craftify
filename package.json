{
  "name": "craftify",
  "displayName": "Craftify",
  "description": "Ship Next.js projects 10x faster using common snippets.",
  "version": "1.1.0",
  "publisher": "MustafaAlhasanat",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MustafaHasanat/craftify"
  },
  "contributes": {
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/nextjs.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/hero-ui.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/hero-ui.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/hero-ui.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/hero-ui.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/tanstack.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/tanstack.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/tanstack.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/tanstack.code-snippets"
      },
      {
        "language": "prisma",
        "path": "./snippets/tools.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/tools.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/tools.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/tools.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/tools.code-snippets"
      }
    ]
  },
  "scripts": {
    "install": "code --install-extension craftify-0.0.1.vsix",
    "package": "vsce package",
    "globalize": "yarn run package & yarn run install",
    "acp": "git add . && git commit -m 'feat: adding features' && git push",
    "publish": "vsce publish"
  }
}
